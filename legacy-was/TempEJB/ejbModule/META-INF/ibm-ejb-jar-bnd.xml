<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar-bnd
        xmlns="http://websphere.ibm.com/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://websphere.ibm.com/xml/ns/javaee http://websphere.ibm.com/xml/ns/javaee/ibm-ejb-jar-bnd_1_2.xsd"
        version="1.2">
	<session name="ConverterBean">
	 	<interface
			class="com.ibm.temp.ejb.ConverterLocal"
			binding-name="ejblocal:ConverterLocal" />
		<interface
			class="com.ibm.temp.ejb.ConverterRemote"
			binding-name="ejb/ConverterRemote" />
	</session>
</ejb-jar-bnd>
